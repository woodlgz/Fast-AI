#include "GA.h"
#include <stdlib.h>

namespace FASTAI{
	namespace GA{

		void Env::evaluate(){
			float accumScore = 0.0;
			for(int i=0;i<m_PSize;i++){
				m_Score[i] = judge(i);
				m_ScoreAux[i] = m_Score[i] + accumScore;
				accumScore = m_ScoreAux[i];
			}
		}

