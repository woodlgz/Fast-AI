#ifndef UTIL_H_
#define UTIL_H_

namespace FASTAI{
	namespace Util{
		namespace Common{
			template<typename T>
			int selectKth(T[] array,int size,int k){
				if(k<=0||k>size)
					return -1;

				return -1;
			}
		}
	};
};

#endif /* UTIL_H_ */
